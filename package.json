{
  "name": "auth0-authz-extension",
  "version": "2.4.0",
  "description": "Auth0 Authorization Extension",
  "engines": {
    "node": "4.6.2"
  },
  "main": "server.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "client:build": "cross-env NODE_ENV=production webpack --config ./build/webpack/config.prod.js --colors -p",
    "serve:dev": "gulp run",
    "serve:prod": "cross-env NODE_ENV=production node index.js",
    "test": "npm run test:pre && cross-env NODE_ENV=test nyc mocha tests/mocha.js ./tests/ --recursive",
    "test:watch": "npm run test:pre && cross-env NODE_ENV=test mocha tests/mocha.js ./tests/ --recursive--watch",
    "test:pre": "npm run test:clean",
    "test:clean": "rimraf ./coverage && rimraf ./.nyc_output",
    "extension:build": "webpack -p --config ./build/extension/webpack.config.js",
    "extension:size": "cross-env NODE_ENV=production webpack -p --config ./build/extension/webpack.config.js --json > ./build/extension/bundle-size.json && node ./build/extension/bundle-size.js",
    "build": "npm run clean && npm run client:build && npm run extension:build && cp ./dist/auth0-authz.extension.$npm_package_version.js ./build/bundle.js",
    "lint:js": "eslint --ignore-path .gitignore --ignore-pattern build --ignore-pattern webpack ."
  },
  "keywords": [
    "auth0"
  ],
  "author": "Sandrino Di Mattia",
  "license": "MIT",

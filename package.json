{
  "name": "auth0-authz-extension",
  "version": "1.4.5",
  "description": "Auth0 Authorization Extension",
  "engines": {
    "node": "5.9.0"
  },
  "main": "server.js",
  "scripts": {
    "clean": "rimraf dist",
    "client:dev": "NODE_ENV=development node ./build/webpack/server.js",
    "client:build": "npm run clean && NODE_ENV=production webpack --config ./build/webpack/config.prod.js --colors -p",
    "server:dev": "NODE_ENV=development nodemon --ignore assets/app/ --ignore build/webpack/ --ignore server/db.json --ignore client/ --ignore node_modules/ index.js",
    "server:prod": "NODE_ENV=production node index.js",
    "test": "NODE_ENV=test mocha -R spec --compilers js:babel-core/register --timeout 5000 $(find ./tests -name *.tests.js) tests/runner.js",
    "test:watch": "NODE_ENV=test mocha -R spec --compilers js:babel-core/register --recursive --timeout 5000 $(find ./tests -name *.tests.js) --watch tests/runner.js",
    "postinstall": "npm run client:build",
    "extension:build": "webpack -p --config ./build/extension/webpack.config.js",
    "extension:size": "NODE_ENV=production webpack -p --config ./build/extension/webpack.config.js --json > ./build/extension/bundle-size.json && node ./build/extension/bundle-size.js",
    "build": "npm run clean && npm run client:build && npm run extension:build"
  },
  "keywords": [
    "auth0"
  ],
  "author": "Sandrino Di Mattia",
  "license": "MIT",
  "dependencies": {
    "async": "~1.5.2",
    "auth0": "^2.1.0",
    "auth0-oauth2-express": "^1.1.7",
    "aws-sdk": "^2.2.47",
    "axios": "^0.9.1",
    "babel": "^6.5.2",
    "babel-core": "^6.7.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "babel-runtime": "^6.6.1",
    "bluebird": "^3.3.4",
    "body-parser": "^1.15.0",
    "ejs": "^2.4.1",
    "express": "^4.13.4",
    "express-jwt": "^3.3.0",
    "glob": "^7.0.3",
    "json-loader": "^0.5.4",
    "lodash": "^3.9.3",
    "lowdb": "^0.12.5",
    "lru-memoizer": "^1.1.0",
    "moment": "^2.12.0",
    "mongo-getdb": "^2.0.0",
    "mongodb-uri": "^0.9.7",
    "morgan": "^1.7.0",
    "nconf": "^0.8.4",
    "node-uuid": "^1.4.7",
    "request": "^2.69.0",
    "semver": "^5.1.0",
    "validate.js": "^0.9.0",
    "webtask-tools": "^1.3.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.4",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "chai": "^3.5.0",
    "classnames": "^2.2.3",
    "css-loader": "^0.23.1",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^4.2.3",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "history": "^2.0.1",
    "immutable": "^3.7.6",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "jwt-decode": "^2.0.1",
    "mocha": "^2.4.5",
    "nodemon": "^1.9.1",
    "postcss-focus": "^1.0.0",
    "postcss-loader": "^0.8.2",
    "postcss-reporter": "^1.3.3",
    "postcss-simple-vars": "^1.2.0",
    "raw-loader": "^0.5.1",
    "react": "^0.14.7",
    "react-bootstrap": "^0.28.4",
    "react-dom": "^0.14.7",
    "react-loader-advanced": "^1.1.2",
    "react-pure-render": "^1.0.2",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.0",
    "react-transform-hmr": "^1.0.4",
    "redux": "^3.3.1",
    "redux-devtools": "^3.1.1",
    "redux-devtools-dock-monitor": "^1.1.0",
    "redux-devtools-log-monitor": "^1.0.5",
    "redux-form": "^4.2.2",
    "redux-logger": "^2.6.1",
    "redux-promise-middleware": "^2.3.3",
    "redux-simple-router": "^2.0.4",
    "redux-thunk": "^2.0.1",
    "request-promise": "^2.0.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.6.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-sources": "^0.1.1",
    "webpack-stats-plugin": "^0.1.1",
    "webtask-bundle": "^1.3.2"
  }
}

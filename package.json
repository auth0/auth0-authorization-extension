    "babel-runtime": "^6.9.2",
    "blipp": "^2.3.0",
    "bluebird": "^3.4.0",
    "body-parser": "^1.15.1",
    "boom": "^3.2.2",
    "ejs": "^2.4.2",
    "glob": "^7.0.3",
    "good": "^7.0.1",
    "good-console": "^6.1.2",
    "hapi": "^13.5.0",
    "hapi-auth-jwt2": "^7.0.1",
    "inert": "^4.0.1",

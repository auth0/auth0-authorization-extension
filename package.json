      "superagent@1.2.0",
      "tough-cookie@2.2.2",
      "webtask-tools",
      "winston@1.0.0"
    ],
    "settings": {
      "WARN_DB_SIZE": 409600,
      "MAX_MULTISELECT_USERS": 5,
      "MULTISELECT_DEBOUNCE_MS": 250,
      "PER_PAGE": 10
    }
  },

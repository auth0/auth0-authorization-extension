{
  "name": "auth0-authz-extension",
  "version": "2.12.0",
  "description": "Auth0 Authorization Extension",
  "engines": {
    "node": ">=12"
  },
  "main": "server.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && npm run client:build && npm run extension:build",
    "client:build": "cross-env NODE_ENV=production webpack --config ./build/webpack/config.prod.js --colors -p",
    "extension:build": "a0-ext build:server ./webtask.js ./dist && cp ./dist/auth0-authz.extension.$npm_package_version.js ./build/bundle.js",
    "serve:dev": "gulp run",
    "serve:prod": "cross-env NODE_ENV=production node index.js",
    "test": "npm run test:pre && cross-env NODE_ENV=test nyc mocha tests/unit/mocha.js ./tests/unit/**/*.tests.js",
    "test:watch": "npm run test:pre && cross-env NODE_ENV=test mocha tests/unit/mocha.js ./tests/unit/**/*.tests.js --watch",
    "test:pre": "npm run test:clean",
    "test:clean": "rimraf ./coverage && rimraf ./.nyc_output",
    "test:integration": "mocha tests/integration/mocha.js ./tests/integration/**/*.tests.js -R list --timeout 12000",
    "lint:js": "eslint --ignore-path .gitignore --ignore-pattern build --ignore-pattern webpack .",
    "lint:fix": "eslint --ignore-path .gitignore --ignore-pattern build --ignore-pattern webpack . --fix"
  },
  "keywords": [
    "auth0"
  ],
  "author": "Sandrino Di Mattia",
  "license": "MIT",
  "auth0-extension": {
    "externals": [
      "async@2.1.2",
      "auth0-extension-hapi-tools@1.3.1",
      "auth0-extension-s3-tools@1.1.1",
      "auth0-extension-tools@1.3.2",
      "auth0@2.14.0",
      "aws-sdk@2.5.3",
      "blipp@2.3.0",
      "bluebird@3.5.0",
      "body-parser@1.12.4",
      "boom@3.2.2",
      "ejs@2.3.1",
      "good-console@6.1.2",
      "good@7.0.1",
      "hapi-auth-jwt2@7.0.1",
      "hapi-swagger@7.4.0",
      "iconv-lite@0.4.10",
      "inert@4.0.1",
      "joi@9.0.4",
      "jsonwebtoken@7.1.9",
      "jwks-rsa@1.1.1",
      "lodash@3.10.1",
      "lru-memoizer@1.10.0",
      "morgan@1.5.3",
      "node-uuid@1.4.3",
      "relish@0.2.4",

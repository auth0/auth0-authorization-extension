{
  "name": "auth0-iam-dashboard",
  "version": "1.0.0",
  "description": "Identity and Access Management Dashboard for Auth0",
  "engines": {
    "node": "5.9.0"
  },
  "main": "server.js",
  "scripts": {
    "clean": "rimraf assets/app",
    "client:dev": "node ./build/exec.js development \"node ./build/webpack/server.js\"",
    "client:prod": "npm run clean && node ./build/exec.js production \"webpack --config ./build/webpack/config.prod.js --progress --colors -p\"",
    "server:dev": "node ./build/exec.js development \"nodemon --ignore assets/app/ --ignore build/webpack/ --ignore server/db.json --ignore client/ --ignore node_modules/ index.js\"",
    "server:prod": "node ./build/exec.js production \"node index.js\"",
    "test": "NODE_ENV=test mocha -R spec --compilers js:babel-core/register --timeout 5000 $(find ./tests -name *.tests.js) tests/runner.js",
    "test:watch": "NODE_ENV=test mocha -R spec --compilers js:babel-core/register --recursive --timeout 5000 $(find ./tests -name *.tests.js) --watch tests/runner.js",
    "postinstall": "npm run build:prod"
  },
  "keywords": [
    "auth0"
  ],
  "author": "Sandrino Di Mattia",
  "license": "MIT",
  "dependencies": {
    "async": "^1.5.2",
    "auth0": "^2.0.0",
    "aws-sdk": "^2.2.26",
    "axios": "^0.9.1",
    "babel": "^6.3.26",
    "babel-core": "^6.3.26",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "bluebird": "^3.1.1",
    "body-parser": "^1.14.2",
    "ejs": "^2.3.4",
    "express": "^4.13.3",
    "lodash": "^4.6.1",
    "lowdb": "^0.12.5",
    "lru-memoizer": "^1.1.0",
    "moment": "^2.11.0",
    "mongo-getdb": "^2.0.0",
    "mongodb-uri": "^0.9.7",
    "morgan": "^1.6.1",
    "nconf": "^0.8.2",
    "node-uuid": "^1.4.7",
    "request": "^2.67.0",
    "validate.js": "^0.9.0",
    "webtask-tools": "^1.2.0",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.2.3",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "chai": "^3.5.0",
    "classnames": "^2.2.2",
    "css-loader": "^0.23.1",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^4.2.1",
    "exports-loader": "^0.6.2",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^0.8.5",
    "history": "^2.0.1",
    "immutable": "^3.7.6",
    "imports-loader": "^0.6.5",
    "jwt-decode": "^1.4.0",
    "mocha": "^2.4.5",
    "nodemon": "^1.8.1",
    "postcss-focus": "^1.0.0",
    "postcss-loader": "^0.8.0",
    "postcss-reporter": "^1.3.0",
    "postcss-simple-vars": "^1.2.0",
    "react": "^0.14.5",
    "react-bootstrap": "^0.28.1",
    "react-dom": "^0.14.3",
    "react-loader-advanced": "^1.1.2",
    "react-pure-render": "^1.0.2",
    "react-redux": "^4.0.6",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.0",
    "react-transform-hmr": "^1.0.1",
    "redux": "^3.0.5",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "redux-form": "^4.1.0",
    "redux-logger": "^2.3.1",
    "redux-promise-middleware": "^2.3.2",
    "redux-simple-router": "^2.0.4",
    "redux-thunk": "^2.0.1",
    "rimraf": "^2.5.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-dev-server": "^1.14.0",
    "webpack-hot-middleware": "^2.6.0",
    "webpack-sources": "^0.1.0",
    "webpack-stats-plugin": "^0.1.1"
  }
}
